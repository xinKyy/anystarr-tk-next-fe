    "antd": "^4.3.0",
    "dynamic-antd-theme": "latest",
    "echarts": "^5.4.3",
    "express": "^4.16.3",
    "i18next": "^21.5.2",
    "i18next-browser-languagedetector": "^6.1.2",
    "if-comp": "^0.0.8",
    "next": "^12.0.6",
    "next-redux-wrapper": "^6.0.2",
    "prop-types": "^15.6.2",
    "query-string": "^6.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^13.5.0",
    "react-redux": "7.2.0",
    "redux": "^4.0.5",
