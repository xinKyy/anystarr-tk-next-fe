{
  "name": "next-antd-scafflod",
  "version": "1.0.0",
  "description": "react + next.js",
  "main": "server.js",
  "author": "xink",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --no-ignore --fix",
      "git add --force"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add --force"
    ]
  },
  "scripts": {
    "start": "node server.js",
    "build": "next build",
    "analyze": "ANALYZE=true next build",
    "prod": "NODE_ENV=production node server.js"
